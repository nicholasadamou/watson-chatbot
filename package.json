    "express-session": "1.17.1",
    "hammerjs": "2.0.8",
    "http-proxy-middleware": "0.19.1",
    "https": "1.0.0",
    "ibm-watson": "5.5.0",
    "jimp": "0.12.1",
    "jsonwebtoken": "8.5.1",
    "moment": "2.25.3",
    "newrelic": "^6.13.1",
    "node-schedule": "1.3.2",
    "nodemon": "1.19.4",
